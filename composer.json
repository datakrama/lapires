{
    "name": "datakrama/lapires",
    "description": "Lapires - Laravel API Response, generalizing all response from your API server",
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "phpro/grumphp": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Husen",
            "email": "hello@husen.id"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Datakrama\\Lapires\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datakrama\\Lapires\\Test\\": "tests"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix --dry-run",
        "csf": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit --verbose"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datakrama\\Lapires\\LapiresServiceProvider"
            ]
        }
    }
}
