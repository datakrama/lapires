{
    "name": "datakrama/lapires",
    "description": "Lapires - Laravel API Response, generalizing all response from your API server",
    "type": "library",
    "require": {
        "illuminate/support": "^6"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.3@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Husen",
            "email": "husenisme@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Datakrama\\Lapires\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datakrama\\Lapires\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "extra": {
        "laravel": {
            "providers": [
                "Datakrama\\Lapires\\LapiresServiceProvider"
            ]
        }
    }
}
